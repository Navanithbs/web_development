<%- include ('partials/header.ejs') -%>
<div class="container">
    <div class="jumbotron">
        <h1><strong>This is Campgrounds page<i class="fas fa-campground"></i></strong></h1>
        <a class="btn btn-lg btn-outline-primary" href="/campground/new">Add new Campground</a>
    </div>
    <div class="row">
        <% campground.forEach(function(campground){%>
            <div class="col-lg-4 col-md-6 col-sm-6">
                <h3><%=campground.name%></h3>
                <img src="<%=campground.img%>">
            </div>
            <% })%>
    </div>
<%- include ('partials/footer.ejs') -%>
